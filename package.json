{
  "name": "marsh",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "electron-main.cjs",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "start:electron": "npm run build && electron .",
    "electron": "electron .",
    "lint": "eslint .",
    "preview": "vite preview",
    "package": "npm run build && electron-builder build --mac --dir"
